{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://schemas.3leaps.dev/sysprims/process/v1.0.0/port-filter.schema.json",
  "title": "sysprims port filter",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "protocol": {
      "type": "string",
      "enum": ["tcp", "udp"]
    },
    "local_port": {
      "type": "integer",
      "minimum": 1,
      "maximum": 65535
    }
  }
}
